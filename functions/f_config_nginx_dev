server {
      listen 80;
      listen [::]:80;

      server_name busydev;

      root /var/www/busydev;
      location / {
            index index.html;
      }
      location /transmission {
            proxy_pass http://busydev:9091/transmission;
      }
      location /busyfin {
            proxy_pass http://192.168.50.230:8096/web;
      }
}
