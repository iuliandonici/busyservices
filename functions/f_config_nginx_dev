server {
      listen 80;
      listen [::]:80;

      server_name busydev.router.ho.me;
      root /var/www/busydev;
      index index.html;

      location / {
            try_files $uri $uri/;
      }
      location /jellyfin {
            proxy_set_header X-Real-IP  $remote_addr;
            proxy_set_header X-Forwarded-For $remote_addr;
            proxy_set_header Host $host;
            proxy_pass http://192.168.50.230:9091/transmission/web/;
      }
}
