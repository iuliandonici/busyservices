server {
      listen 80;
      listen [::]:80;

      server_name busydev.router.ho.me;
      root /var/www/busydev;
      index index.html;

      location / {
              try_files $uri $uri/ =404;
      }
      location jellyfin {
            proxy_pass http://192.168.50.230:8096/web/;
      }
}
