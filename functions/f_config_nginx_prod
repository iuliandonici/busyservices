upstream busyfin {
      server 127.0.0.1:8096;
}
upstream busytrans {
      server 192.168.50.10:9091;
}
server {
      listen 80;
      listen [::]:80;
      server_name busycenter;
      root /var/www/busyprod;
      
      location / {
            index index.html;
      }
      location /busytrans {
            proxy_pass http://busytrans;
      }
      location /busyfin {
            proxy_pass http://busyfin;
      }
}
