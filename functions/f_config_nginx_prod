server {
      listen 80;
      listen [::]:80;

      server_name busycenter;

      root /var/www/busyprod;
      location / {
            index index.html;
      }
      location /transmission {
            proxy_pass $server_name:9091/transmission;
      }
      location /busyfin {
            proxy_pass http://192.168.50.230:8096/web;
      }
}
