server {
      listen 80;
      listen [::]:80;

      server_name busyprod;

      root /var/www/busyprod;
      index index.html;
      location / {
            index index.html;
      }
      location /busytrans {
            proxy_pass http://192.168.50.230:9091/transmission;
      }
      location /busyfin {
            proxy_pass http://192.168.50.230:8096/web;
      }
}
